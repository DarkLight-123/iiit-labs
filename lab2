def greet(name):
    print("Привет, " + name + "!")

def square(number):
    return number ** 2

def max_of_two(x, y):
    return x if x > y else y

def describe_person(name, age=30):
    print("Имя: " + name + ", Возраст: " + str(age))

def is_prime(number):
    for i in range(2, number ** 0.5 + 1):
        if number % i == 0:
            return False
    return True


